<div id="clip-list">
  <% clips.each do |clip| %>
    <div data-clip-id="<%= clip.id %>" >
      <p>Start:<span class="play-clip" data-start="<%= clip.start_time %>"><%= clip.seconds_to_hms(clip.start_time) %></span></p>
      <p>End:<span class="play-clip" data-start="<%= clip.end_time %>"><%= clip.seconds_to_hms(clip.end_time) %></span></p>
      <div>
        <button class="play-clip" data-start="<%= clip.start_time %>">再生</button>
        <button class="delete-clip">削除</button>
      </div>
    </div>
  <% end %>
</div>


